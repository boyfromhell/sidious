{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-cors": "^0.11.2",
        "christophrumpel/nova-notifications": "^1.0",
        "emilianotisato/nova-tinymce": "^0.5.4",
        "fideloper/proxy": "^4.0",
        "kalnoy/nestedset": "^4.3",
        "laravel/framework": "5.8.*",
        "laravel/nova": "*",
        "laravel/tinker": "^1.0",
        "lorisleiva/laravel-actions": "^0.1.2",
        "my-com/laravel-conditional-actions": "^1.0@beta",
        "predis/predis": "^1.1",
        "silber/bouncer": "v1.0.0-rc.5",
        "spatie/laravel-medialibrary": "^7.6",
        "spatie/laravel-sluggable": "^2.1",
        "spatie/laravel-tags": "^2.4",
        "tymon/jwt-auth": "1.0.0-rc.4.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.5",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "hydreflab/jedi-faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\Forum\\": "modules/forum/src",
            "Modules\\Galaxy\\": "modules/galaxy/src",
            "Modules\\Economy\\": "modules/economy/src",
            "Modules\\Personnages\\": "modules/personnages/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Modules\\Forum\\Tests\\": "modules/forum/tests",
            "Modules\\Galaxy\\Tests\\": "modules/galaxy/tests",
            "Modules\\Economy\\Tests\\": "modules/economy/tests",
            "Modules\\Personnages\\Tests\\": "modules/personnages/tests"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ]
}
